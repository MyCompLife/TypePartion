procedure LoadPostPoneDoc(PostPoneDoc:ICDocuments);
begin
 try
   TblContents.DisableControls;
   TblContents.Clear;
   PostPoneDoc.AppendLinesTo('Товар;Количество;Цена;ЦенаБезСкидкиНадбавки;ПроцентСкидкиНадбавки;КомментарийСкидкиНадбавки;РучнаяКорректировка;ЦенаИзШтрихКода',TblContents);
   TblContents.DoGetLinks('Код=Товар.Код;ЕдИзм=Товар.ЕдИзм');
   Partner := PostPoneDoc.Партнер;
   SetPartner(Partner); 
   Nil(DiscCards);
   if VarAsBool(PostPoneDoc.@ДисконтКарточка.IsFocused) then
     begin
       DiscCards := CreateObject('Справочники.ДисконтныеКарты');
       if DiscCards.Find(PostPoneDoc.@ДисконтКарточка) then
         edCard.Text := DiscCards.CodeField
       else
         Nil(DiscCards);
     end;
   UpdateAllSum;
 finally
   TblContents.EnableControls;
 end
end;

procedure PostPoneDoc_Click(Sender: TObject); 
var CurPanel : TO4Panel;   
    MessageStr : string;
begin
 CurPanel := Sender as TO4Panel;
 if not IsNil(CurPanel) and PostPoneDocsList.ValidIndex(CurPanel.Tag) then
   begin     
     if (TblContents.LineCount > 0) and not IsObjFocused(PostPoneDoc) then
       MessageStr := 'Текущий чек не сохраниться !!!'+#13+'Перейти к чеку №'
     else
       MessageStr := 'Перейти к чеку №';
     if MessageDlg(MessageStr+PostPoneDocsList.GetStr(CurPanel.Tag)+'?',mtConfirmation,ArrayOf(mbYes,mbNo))=mrYes then
       begin    
         if IsObjFocused(PostPoneDoc) and
           (MessageDlg('Сохранить текущий отложенный чек №'+VarAsStr(PostPoneDoc.GetNumber)+'?',mtConfirmation,ArrayOf(mbYes,mbNo))=mrYes) then
             CreateNaklDoc(true);
         Nil(PostPoneDoc);
         PostPoneDoc := CreateObject('Документы.Чек');
         PostPoneDoc.Select;
         if PostPoneDoc.Find(PostPoneDocsList.GetValue(CurPanel.Tag)) then 
           begin 
             LoadPostPoneDoc(PostPoneDoc); 
             RefreshPostPoneDocsList;
             AddPostPoneDocToPanel;
           end
         else
           ShowMessage('Документ не найден!');
      end;

   end;
end;

