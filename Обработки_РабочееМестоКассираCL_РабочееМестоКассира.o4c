procedure miCard_Click(Sender: TObject);
var
  PartCode, FieldName: String;
  Accept: Boolean;
begin   
  if BlockChangePartner then
    begin
      ShowMessage('Вам запрещено изменять партнера!'); 
      edCard.Text := '';
      exit;
    end;
  Accept := true;
