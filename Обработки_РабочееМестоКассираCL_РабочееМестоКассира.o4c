procedure AddPostPoneDocToPanel;
var i, IBegin, IEnd, iDocPanel : integer;
    CurDocPanel : TO4Panel; 
    iDocPanelStr : string;
begin
 if PostPoneDocsList.Count>0 then
   begin     
     IBegin := PostPoneDocPanelPage*MaxPagePostPoneDocCount;
     While IBegin>=PostPoneDocsList.Count do
       begin
         IBegin := IBegin-MaxPagePostPoneDocCount;
         if IBegin<0 then IBegin := 0;
       end;
     IEnd := IBegin + MaxPagePostPoneDocCount -1;
     if IEnd>=PostPoneDocsList.Count then
       IEnd := PostPoneDocsList.Count-1;
     iDocPanel := 1;
     for i:= IBegin to IEnd do
       begin
         iDocPanelStr := IntToStr(iDocPanel);    
         CurDocPanel := Form.FindComponent('PostPoneDocNumber'+iDocPanelStr) as TO4Panel;
          if not IsNil(CurDocPanel) then 
            begin
              CurDocPanel.Caption := ExtractWord(1,PostPoneDocsList.GetStr(i),#13); 
              CurDocPanel.Tag := i;
            end;

         CurDocPanel := Form.FindComponent('PostPoneDocSum'+iDocPanelStr) as TO4Panel;
          if not IsNil(CurDocPanel) then            
            begin
              CurDocPanel.Caption := ExtractWord(2,PostPoneDocsList.GetStr(i),#13);
              CurDocPanel.Tag := i;
            end;

         CurDocPanel := Form.FindComponent('PostPoneDocPartner'+iDocPanelStr) as TO4Panel;
          if not IsNil(CurDocPanel) then   
            begin
              CurDocPanel.Caption := ExtractWord(3,PostPoneDocsList.GetStr(i),#13);
              CurDocPanel.Tag := i;   
            end;
         CurDocPanel := Form.FindComponent('PostPoneDoc'+iDocPanelStr) as TO4Panel;
         if not IsNil(CurDocPanel) then
           begin
             CurDocPanel.Visible := true;
             if IsObjEQ(PostPoneDoc,PostPoneDocsList.GetValue(i)) then 
               begin
                 CurDocPanel.BevelOuter := bvLowered;
                 CurDocPanel.Color := clSilver;
               end
             else
               begin
                 CurDocPanel.BevelOuter := bvRaised; 
                 CurDocPanel.Color := clBtnFace;
               end;
           end;
         inc(iDocPanel);
       end;
       // Прячем незадействованные панели
     if iDocPanel<=MaxPagePostPoneDocCount then
       for i:=MaxPagePostPoneDocCount  downto iDocPanel do
         begin
           CurDocPanel := Form.FindComponent('PostPoneDoc'+IntToStr(i)) as TO4Panel;
           if not IsNil(CurDocPanel) then
             begin
               CurDocPanel.Visible := false; 
               CurDocPanel.Tag := -1;
             end;
         end;
     tbPostPoneLeft.Enabled := IBegin>0;
     tbPostPoneRight.Enabled := IEnd<(PostPoneDocsList.Count-1);
     pnPostPoneDocs.Visible := true;
   end
 else
   pnPostPoneDocs.Visible := false;
end;

procedure RefreshPostPoneDocsList;
var Jrn : ICJournal;
    CurDoc : ICDocuments;
begin
 PostPoneDocsList.Clear;
 PostPoneDocsList.SortByStr(true);
 if IsObjFocused(ChangeDoc) then
   begin
     Jrn := CreateObject('Журналы.ЖурналЧеков');
     Jrn.SetFieldFilter('Смена','=',ChangeDoc,'','',Null);
     Jrn.SetDocStateRange(0);                              
     Jrn.SortByField('НомерДокумента');
     Jrn.Select;
     While Jrn.SelectNext do
       begin
         CurDoc := Jrn.GetDoc;
         if not VarAsBool(CurDoc.ЗакрытЧек) then
           PostPoneDocsList.AddValue('№'+CurDoc.GetNumber+#13+DecToStr(VarAsDec(CurDoc.Сумма),2)+#13+
                                     StrCopy(VarAsStr(CurDoc.Партнер.NameField),1,24),CurDoc);
       end;
    PostPoneDocsList.SortByStr(true);
   end;
end;

