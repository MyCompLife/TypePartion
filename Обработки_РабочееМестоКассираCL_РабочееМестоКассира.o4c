function CreateNaklDoc(IsPostPoneDoc:Boolean=false):boolean;
var DelAckValue, IsNewDoc : boolean;  
    ChildPayJrn : ICJournal;
begin 
 Result := false;
 if IsObjFocused(PostPoneDoc) then
   begin
     PostPoneDoc.Refresh;
     if PostPoneDoc.GetDocState>0 then
       begin
         if MessageDlg('Отложеный документ №'+VarAsStr(PostPoneDoc.GetNumber)+' уже зафиксирован!'+#13+
                       'Его кто-то изменил и зафиксировал в журнале чеков!'+#13+
                       'Создать новый документ?',mtConfirmation,Arrayof(mbYes,mbNo))=mrYes then
           begin
             Nil(PostPoneDoc);
           end
         else
           begin
             ShowMessage('Создание документа отменено!');
             exit;
           end;
       end
     else
       begin
         ChildPayJrn := CreateObject('Журналы.ЖурналПодчПлатежейОтп');
         ChildPayJrn.UseMaster(PostPoneDoc);
         if ChildPayJrn.Select then
           begin
             ShowMessage('По отложеному чеку '+VarAsStr(PostPoneDoc.GetNumber)+' есть оплаты!'+#13+
                         'Удалите оплаты!');
             exit;
           end;
       end;
   end;
  try
    TblContents.DisableControls;
    NaklDoc.Select;
    if IsObjFocused(PostPoneDoc) and NaklDoc.Find(PostPoneDoc) then
      begin  
        IsNewDoc := false;
        NaklDoc.Edit;
        NaklDoc.ClearContents;
      end
    else
      begin            
        IsNewDoc := true;
        NaklDoc.Append;
      end;
    NaklDoc.Партнер := Partner; 
    if IsObjFocused(AnalitPartnerDic) then
     NaklDoc.АналитикаВзаиморасчета := AnalitPartnerDic;
    NaklDoc.Посредник := Agent;
    NaklDoc.ПроцСкидки := PartnerDiscountPrecent;
    if Store.IsFocused then
      NaklDoc.СкладПоУмолч := Store;
    if Region.IsFocused then
      NaklDoc.Регион := Region;
    if Employ.IsFocused then
      NaklDoc.Ответственный := Employ;
    NaklDoc.Отпуск := 1;
    NaklDoc.ТипОплаты := PayType;
    NaklDoc.Статья := ParamsSt.@Чек;
    NaklDoc.LoadContents('Товар;Количество;Цена;ЦенаБезСкидкиНадбавки;ПроцентСкидкиНадбавки;КомментарийСкидкиНадбавки;РучнаяКорректировка;ЦенаИзШтрихКода',TblContents);
    if IsObjFocused(DiscCards) then
      begin
        NaklDoc.ДисконтКарточка :=  DiscCards;
        //NaklDoc.ТипДисконтнойКарты := DiscCards.@ТипДисконтнойКарты;
      end;
    NaklDoc.КолОбщ := RoundDec(TblContents.Total('Количество'),5);
    NaklDoc.Сумма := RoundDec(TblContents.Total('СуммаСтроки'),2);
    NaklDoc.Валюта := DocCurrency;
    NaklDoc.Курс := DocCourse;
    if DocCourse<>0 then
      NaklDoc.СуммаВВалюте := NaklDoc.Сумма  / DocCourse;
    NaklDoc.Смена := ChangeDoc;
    NaklDoc.Заметки := mComment.text;   
    if IsNewDoc then
      begin
        NaklDoc.BaseID := VarAsInt(Constants.КодИБ);
        NaklDoc.NumID := УстановитьНомерДокумента(NaklDoc, '');
      end;
    NaklDoc.ЗакрытЧек := not IsPostPoneDoc;
    NaklDoc.Post;
    NaklDoc.ApplyUpdates;
    Result := true;
    if not IsPostPoneDoc then
      begin
        NaklDoc.Params['NotCheckRestrictions'] := true;
        NaklDoc.StateUp;
        if NaklDoc.GetDocState<1 then
          try
            DelAckValue := Options.GetServerPrm(DelAck);
            Options.SetServerPrm(DelAck,false);
            NaklDoc.Delete;
            Result := false;
          finally
            Options.SetServerPrm(DelAck,DelAckValue);
          end;
      end;
    Nil(PostPoneDoc);
  finally
    TblContents.EnableControls;
  end;
end;

