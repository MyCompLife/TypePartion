         end;
       AcquiringIncomeDoc.СуммаКомиссии := RoundDec(AcquiringIncomeDoc.Сумма*AcquiringIncomeDoc.ПроцентКомиссии/100, 2);
       AcquiringIncomeDoc.СуммаЗачисления := AcquiringIncomeDoc.Сумма - AcquiringIncomeDoc.СуммаКомиссии;
