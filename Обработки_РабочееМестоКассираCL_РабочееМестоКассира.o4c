procedure edAgent_ActionExecute(Sender: TObject);
var DefName : string;
    SelAgent : ICDictionary;
begin
  SelAgent := CreateObject('Справочники.Служащие');
  if SelAgent.SelectInForm('',DefName,Null) then
    begin
      if IsNil(Agent) then
        Agent := CreateObject('Справочники.Служащие');
      if Agent.Find(SelAgent) then
        edAgent.Text := DefName;
    end;
end;

Procedure AddToLog(TypeOper:Integer; EmplOper, Source : Variant; Count,Summa : Decimal);
Var
  LogDic : IcDictionary; 
  Doc : IcDocuments;
Begin
  LogDic := CreateObject('Справочники.ЛогРабочегоМеста');
  LogDic.Append;
  LogDic.Дата := CurrentDateTime;
  LogDic.Ответственный := EmplOper;
  LogDic.Действие := TypeOper;
  LogDic.Количество := Count;
  LogDic.Сумма := Summa;
  Case TypeOper of
    0: LogDic.Коментарий := 'Добавление товара: '+ VarAsStr(Source.NameField);
    1: LogDic.Коментарий := 'Удаление товара: '+ VarAsStr(Source.NameField);
    2: LogDic.Коментарий := 'Изменение количества товара: '+ VarAsStr(Source.NameField);
    3: LogDic.Коментарий := 'Закритие чека: №'+ VarAsStr(Source.GetNumber)+' от ' +DateToStr(Source.GetDate);
  end;
  LogDic.Post;
  LogDic.ApplyUpdates;
end;

