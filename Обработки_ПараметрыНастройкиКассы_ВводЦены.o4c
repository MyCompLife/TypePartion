interface

uses
  System, DispIntf, Расчеты, РаботаСОбъектами, РаботаСПеременными,
  РаботаСТаблицамиЗначений;
Var
  Good : ICDictionary;
  CalcNum, FirstNum : Integer;
  V: ICProcessing;
implementation

procedure Form_Execute(Sender: TObject; Mean: Variant; Params: Variant);       
begin
  V := Mean;
  edCntTablo.text := VarAsStr(V.Params['Price']);
  lbGoodName.Caption := VarAsStr(V.Params['GoodName']);
end;

procedure btOk_Click(Sender: TObject);
begin
  V.Params['NewPrice'] := VarAsDec(edCntTablo.text)
end;

end.
