procedure Structure1_RT_GetData(Table:TRBTable; Index,Count:Integer; var Accept:Boolean);
begin
  StoreTbl.SortBy('—умќбщ');
  if Index=0 then Accept := StoreTbl.SelectFirst()
             else Accept := StoreTbl.SelectNext();
  if not Accept then Exit;
  with Table do begin
   Value['Name'] := StoreTbl.—клад;
   Value['SUMMAHD'] := StoreTbl.—умќбщ¬ал;
   Value['CurrName'] := StoreTbl.¬алюта. од;
   Value['SUMMA'] := StoreTbl.—умќбщ;
  end;
end;

