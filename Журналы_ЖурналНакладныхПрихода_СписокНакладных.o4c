procedure miFillTaxGroup_Click(Sender: TObject);
var P : ICProcessing;
    Doc : ICDocuments;
begin
 if MasterData.IsFocused then
   begin
      Doc := MasterData.GetDoc;
      P := CreateObject('Обработки.Диалоги');
      if Doc.SetSrvToClientPos and (P.Execute('ВыборНалоговойГруппыТовара',Null)=mrOK) and P.HasParam('TaxGroup') then
        begin
          if MasterData.RunThreadProcess(server.FillGoodTaxGroupEP(Doc.SrvMean,VarAsInt(P.Params['TaxGroup'])),
                                         'Заполнение налоговой группы') then
            begin
              ShowMessage('Налоговоя группа заполнена!');
            end
          else
            ShowMessage('Процесс прерван пользователем либо произошла ошибка!');
        end;
   end;
end;

