function SoftOpenTable(var Table: Variant; FilePath: String): Boolean;
var ErrStr: String;
begin
  try
    Table := CreateOLEObject('s4DBtabl.S4VPADBTable');
    Table.TableName := FilePath;
    ErrStr := Trans('Открыта таблица ')+ FilePath;
    Result := True;
  except
    Nil(Table);
    ErrStr := Trans('Ошибка при открытии таблицы ')+FilePath+' !'#13#10+ExceptMessage;
    Result := False;
  end;
end;

procedure CreateDocs(DocName, DbfFieldHeadName, DbfFieldListName, FilePath : String; var Log: TStringList);
Var
  Docs, OwnerDoc : ICDocuments;
  OwnerDic, Currency : ICDictionary;
  DocsJourn : ICJournal;
  AllRecCnt, RecCurrent, ColumnCnt, CntWord : Integer;
  OwnerDocSign, OwnerDicSign, StructurTbl, Str : String;
  DbfTable: Variant;
begin
  CreateObjects('ЧекВНаклПрихода', DocsTbl, DocsListTbl);
  //-----------------------Распаковка таблицы из файла--------------------------
  if SoftOpenTable(DbfTable, FilePath) then
    begin
     if VarAsBool((not IsNil(DbfTable)) and DbfTable.Active) then
       begin
         DbfTable.First;
         if VarAsBool(DbfTable.HasField(DbfFieldHeadName)) then
           DocsTbl.PackedData := DbfTable._Default[DbfFieldHeadName];
         if ((DbfFieldListName <> '') and VarAsBool(DbfTable.HasField(DbfFieldListName))) then  //DbfFieldListName = '' используется в случаи, если у документа нет табл. части
           DocsListTbl.PackedData := DbfTable._Default[DbfFieldListName];
       end;
    end;
  if DocsTbl.LineCount = 0 then
    exit;
  //------------------------Заполнение Link - полей ----------------------------
  if DbfFieldHeadName <> '' then
    AppendLinkRecords(DocsTbl,'Head',DocName,pStatusOpers,Log);
  if DbfFieldListName <> '' then
    AppendLinkRecords(DocsListTbl,'List',DocName,pStatusOpers,Log);
  //----------------------------------------------------------------------------_________________________________
  AllRecCnt := DocsTbl.LineCount;
  Docs := CreateObject('Документы.' + DocName);
  Docs.Select;
  //------------Перекидка записей шапки из таблицы значений в документ----------
  DocsTbl.Select;
  While DocsTbl.SelectNext Do
    begin
      Docs.Append;
//----------------Создание структуры полей для перекидки в БД-----------------
      StructurTbl := StringColumnsWithoutDelIndex(DocsTbl);
      Docs.AssignFields(StructurTbl,DocsTbl);
      Docs.Ответственный := GetEmplByName(GetUserName);
      Docs.Партнер := Constants.ОсновнойПоставщик;
      Docs.Регион := GetUsersField('Регион');
      Docs.СкладПр := GetUsersField('Склад');
      Docs.Отпуск := 0;
      AddInfoDocsToLog('Add', DocName, DocsTbl, 'Head', Log);
//-----------------------Заполнение содержимого документа---------------------
      if (DbfFieldListName <> '') and VarAsBool(Docs.IsFocused) then
        begin
          DocsListTbl.SortBy('DocID_D');
          DocsListTbl.SetRange(DocsTbl.DocID_D, DocsTbl.DocID_D);
          //--------------Создание структуры полей для перекидки в БД---------------
          StructurTbl := StringColumnsWithoutDelIndex(DocsListTbl);
          DocsListTbl.Select;
          While DocsListTbl.SelectNext do
            begin
              Docs.AppendLine;
              Docs.AssignFields(StructurTbl,DocsListTbl);
              Docs.ЦенаБезСкидкиНадбавки := Docs.Цена;
              Docs.ЦенаБезНДС := Docs.Цена;
              Docs.СтавкаНДС := 0;
              Docs.НДС := Docs.Цена - Docs.ЦенаБезНДС;
              Docs.КурсТовара := Docs.Курс;
              Docs.ВалютаТовара := Docs.Валюта;
              if Docs.КурсТовара = 0 then //исключение деления на 0
                begin
                  Docs.КурсТовара := 1;
                  Currency := CreateObject('Справочники.Валюты');
                  Docs.ВалютаТовара := GetNatCurrency(Currency);
                  Docs.ЦенаВВалюте := Docs.Цена;
                end;
              Docs.ЦенаВВалюте := Docs.Цена/Docs.КурсТовара;
              Docs.Склад := Docs.СкладПр;
              AddInfoDocsToLog('Add', DocName, DocsListTbl, 'List', Log);
              Docs.PostLine;
            end;
        end;
      //----------------------------------------------------------------------------
      Docs.Post;
      MasterData.Refresh;
//      if Docs.GetDocState = 0 then
//        Docs.StateUp;
    end;
  StatusText('');
end;


procedure miLoadFromFtpFile_Click(Sender: TObject);
var
  res: Integer;
  FtpDir, LocalDir, File, FileList, ResMeesage: String;
  HLog, DLog : TStringList;
  Accept: Boolean;
  V : ICProcessing;
  TimeFrom: DateTime;
  Protokol, CurDoc : ICDocuments;
  Step : Integer;
  DocsTbl, DocsListTbl: ICValueTable;
begin
  Constants := CreateObject('Константы');
  if not VarAsBool(Constants.ОсновнойПоставщик.IsFocused) then
    begin
      ShowMessage('Не указан поставщик в меню: Параметры -> Настройка обмена');
      exit;
    end;
  try
    pStatusOpers.Visible := true;
    TimeFrom := CurrentDateTime;
    Accept := true;
    HLog := CreateStringList;
    DLog := CreateStringList;
//    DLog.Add(VarAsStr(Step) + ') Соединение с ftp сервером - '+ GetFtpIP);
//    Accept := InitializationFtpConnect(ResMeesage);
    DLog.Add(ResMeesage);
    if not Accept then
      exit;

    FtpDir := '/' + VarAsStr(Constants.КодИБ) + '/';
    Inc(Step);
    DLog.Add(VarAsStr(Step) + ') Получение списка файлов с ftp сервера в дириктории: ' + FtpDir);
    ResMeesage := GetFileList(FtpDir);
    DLog.Add(ResMeesage);
    Accept := ResMeesage <> 'Директория не найдена';
    if not Accept then
      exit;

    Inc(Step);
    DLog.Add(VarAsStr(Step) + ') Выбор файла:');
    V := CreateObject('Обработки.РаботаСFtp');
    V.Params['MaskFile'] := '.dbf';
    if V.Execute('СписокФайлов',ResMeesage) <> mrOk then
      begin
        DLog.Add('Выбор файла отменен, процесс импорта прерван.');
        Accept := false;
      end;
    if not Accept then
      exit;
    // Нужно VarAsStr(V.Params['File']); разбить на файл и дирикторию
    FileList := VarAsStr(V.Params['File']);
    File := ExtractWord(WordCount(FileList,'/'),FileList,'/');
    FtpDir := StrCopy(FileList,1,StrLength(FileList) - StrLength(File));
    DLog.Add(File);

    Inc(Step);
    LocalDir := ExtractFilePath(RunPath)+'Tempftp\';
    DLog.Add(VarAsStr(Step) + ') Проверка локальной директории: ' + LocalDir);
    if not DirectoryExists(LocalDir) then
      MkDir(LocalDir);
    if not Accept then
      exit;

    Inc(Step);
    DLog.Add(VarAsStr(Step) + ') Копирование файла с ftp-сервера: ' + FtpDir + File);
    Accept := CopyFileFromFtp(LocalDir + File, FtpDir + File, ResMeesage);
    DLog.Add(ResMeesage);
    if not Accept then
      exit;
    Accept := CopyFileFromFtp(LocalDir + StrCopy(File,1,StrLength(File)-4) + '.DBT', FtpDir + StrCopy(File,1,StrLength(File)-4) + '.DBT', ResMeesage);
    DLog.Add(ResMeesage);
    if not Accept then
      exit;

    Inc(Step);
    DLog.Add(VarAsStr(Step) + ') Заполнение документа из файла: ' + LocalDir + File);
    CreateDocs('НакладнаяПрихода', 'HeadDoc', 'ListDoc', LocalDir + File, DLog);

//    ParsFile(LocalDir + V.Params['File']);

    Inc(Step);
    DLog.Add(VarAsStr(Step) + ') Удаление файла с ftp-ресурса: ' + FtpDir + File);
    Accept := DeleteFileFromFtp(FtpDir + File, ResMeesage);
    DLog.Add(ResMeesage);
    if not Accept then
      exit;
    Accept := DeleteFileFromFtp(FtpDir + StrCopy(File,1,StrLength(File)-4) + '.DBT', ResMeesage);
    DLog.Add(ResMeesage);
    if not Accept then
      exit;

    ResMeesage := 'Импорт выполнен успешно';
    DLog.Add(ResMeesage);

  finally
    if not Accept then
      DLog.Add('******************** Импорт выполнен с ошибками ********************');
    HLog.Add('Ответственный: '+ GetUserName);
    HLog.Add('Файл '+ LocalDir + File);
    HLog.Add('Время начала '+ VarAsStr(TimeFrom));
    HLog.Add('Время окончания '+ VarAsStr(CurrentTime));
//    HLog.Add('Работа с документом: '+ VarAsStr(CurDoc.GetName) + ' №' + VarAsStr(CurDoc.GetNumber));
    Protokol := CreateObject('Документы.ПротоколОбмена');
    Protokol.Select;
    Protokol.Append;
    Protokol.ЭкспортИмпорт := 'Импорт';
    Protokol.Шапка := HLog.Text;
    Protokol.Документы := DLog.Text;
//    Protokol.Ошибки := ELog;
    Protokol.Post;
    Protokol.ApplyUpdates;
    if Accept then
      ResMeesage := 'Импорт выполнен успешно!'
    else
      ResMeesage := 'Импорт прерван или выполнен с ошибками!';
    V := CreateObject('Обработки.РаботаСFtp');
    if V.Execute('ЗавершениеПроцесса',ResMeesage) = mrOk then
      Protokol.EditInForm('протокол',2);
    ftpManagerClose;
    pStatusOpers.Visible := false;
  end;
end;

