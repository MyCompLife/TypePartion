  MasterData.–азрешитьѕрием“овара¬–абћесте := cbUseNaklInDood.Checked;


  s:='';
  for i:=0 to clbRegionTypeDoc.Items.Count-1 do
    if clbRegionTypeDoc.Checked[i] then
      s:=s+IntToStr(i+1)+';';
