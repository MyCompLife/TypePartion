procedure GetJournalParamsList;
var
  i : integer;
  CreateNewTbl, DocVisible, DocState0, DocState1, DocState2: Boolean;
  BaseTbl: ICValueTable;
  FieldsStr: String;
begin
  BaseTbl := CreateObject('ValueTable');
  BaseTbl.AddColumn('Ќом—троки',vtcInteger, 0);
  BaseTbl.AddColumn('“ипƒокумента',vtcString, 100);
  BaseTbl.AddColumn('Ќазв раткоеƒокумента',vtcString, 50);
  BaseTbl.AddColumn('ќтображениеƒокумента',vtcString, 50);
  BaseTbl.AddColumn('Ќабор—осто€нийƒокумента',vtcString, 50);
  BaseTbl.Open;
  CheckTabSheets;
  RealPrmTbl := CreateTblFromBLOB(MasterData.–еал“аблица—Ќастройками∆урнала); //распаковка таблицы
  if IsNil(RealPrmTbl) then
    begin
      CreateNewTbl := true;
    end
  else
    begin //проверка структуры таблицы
      for i := 0 to BaseTbl.GetColumnCount-1 do
        if RealPrmTbl.HasField(BaseTbl.GetColumnName(i)) then //проверка на совпадение названий полей
          begin //проверка на совпадение типов полей
            if RealPrmTbl.GetColumnDataType(RealPrmTbl.GetColumnIndex(BaseTbl.GetColumnName(i))) =
              BaseTbl.GetColumnDataType(i) then
                FieldsStr := FieldsStr + ';' + BaseTbl.GetColumnName(i);
          end
        else
          CreateNewTbl := true;
    end;
  if FieldsStr = '' then
    CreateNewTbl := true
  else
    StrDelete(FieldsStr, 1, 1); // удал€ем первый символ ";"
  if CreateNewTbl then
    begin
      RealPrmTbl := CreateObject('ValueTable');
      RealPrmTbl.Open;
      BaseTbl.CopyTo('',RealPrmTbl);
    end
  else
    begin
      RealPrmTbl.AppendTo(FieldsStr, BaseTbl);
      RealPrmTbl.Clear;
      BaseTbl.CopyTo('',RealPrmTbl);
    end;
  RealPrmTbl.Select;
  RealPrmTbl.SortBy('Ќом—троки');
  While RealPrmTbl.SelectNext do
    begin
      (form.FindComponent('cbDocStore'+IntToStr(RealPrmTbl.Ќом—троки)) as TO4CheckBox).Checked := RealPrmTbl.ќтображениеƒокумента;
      (form.FindComponent('cbDocState'+IntToStr(RealPrmTbl.Ќом—троки)+'_0') as TO4CheckBox).Checked := VarAsBool(ExtractWord(1,RealPrmTbl.Ќабор—осто€нийƒокумента,';'));
      (form.FindComponent('cbDocState'+IntToStr(RealPrmTbl.Ќом—троки)+'_1') as TO4CheckBox).Checked := VarAsBool(ExtractWord(2,RealPrmTbl.Ќабор—осто€нийƒокумента,';'));
      (form.FindComponent('cbDocState'+IntToStr(RealPrmTbl.Ќом—троки)+'_2') as TO4CheckBox).Checked := VarAsBool(ExtractWord(3,RealPrmTbl.Ќабор—осто€нийƒокумента,';'));
    end;
end;

